{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:css && remix build",
    "build:css": "tailwindcss -m -c ./configs/tailwind.js -i ./styles/app.css -o ./app/styles/app.css",
    "deploy": "wrangler publish",
    "dev": "remix build && run-p \"dev:*\"",
    "dev:css": "tailwindcss -w -c ./configs/tailwind.js -i ./styles/app.css -o app/styles/app.css",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev:remix": "remix watch",
    "lint": "eslint .",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.6.5",
    "@remix-run/cloudflare-workers": "^1.6.5",
    "@remix-run/react": "^1.6.5",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@remix-run/dev": "^1.6.5",
    "@remix-run/eslint-config": "^1.6.5",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "miniflare": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
